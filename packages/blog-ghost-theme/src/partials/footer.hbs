<footer id="colophon" class="site-footer">
  <div class="inner">

    {{!-- The tag below includes the theme footer widget area - partials/footer-widgets.hbs --}}
    {{> footer-widgets}}

    <div class="footer-bottom">
      <div class="social-links">
        <a href="{{twitter_url}}" target="_blank">
          <i class="fa-twitter" aria-hidden="true"></i>
          <span class="screen-reader-text">Twitter</span>
        </a>
        <a href="https://github.com/javamonn/trashed" target="_blank">
          <i class="fa-github" aria-hidden="true"></i>
          <span class="screen-reader-text">GitHub</span>
        </a>
      </div><!-- .social-links -->
      <div class="site-info">
        &copy; {{date format="YYYY"}} <a href="{{@site.url}}">{{@site.title}}</a>
      </div><!-- .site-info -->
			<a href="#page" id="top-link" class="top-link"><i class="fa-arrow-left-custom" aria-hidden="true"></i> <span class="top-link-text">Up</span></a>
		</div><!-- .footer-bottom -->

	</div><!-- .inner -->
</footer><!-- .site-footer -->
