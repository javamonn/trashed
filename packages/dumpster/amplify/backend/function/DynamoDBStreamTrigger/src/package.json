{
  "name": "DynamoDBStreamTrigger",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/api": "^2.1.2",
    "@aws-amplify/auth": "^2.1.2",
    "@aws-amplify/core": "^2.2.1",
    "@aws-amplify/pubsub": "^2.1.1",
    "@glennsl/bs-json": "^5.0.1",
    "aws-sdk": "^2.597.0",
    "bs-platform": "^7.0.1",
    "@baransu/graphql_ppx_re": "^0.4.6",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "bsb:build": "bsb -make-world",
    "bsb:start": "bsb -make-world -w",
    "bsb:clean": "bsb -clean-world",
    "graphql:sync-schema": "./scripts/graphql-sync-schema.sh && npm run bsb:clean",
    "webpack:start": "webpack --watch",
    "webpack:build": "npm run bsb:clean && npm run bsb:build && webpack",
    "start": "concurrently -k -n \"bsb,webpack\" \"npm run bsb:start\" \"dotenv npm run webpack:start\"",
    "build:development": "dotenv npm run webpack:build",
    "build": "NODE_ENV=production dotenv npm run webpack:build"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12"
  }
}
